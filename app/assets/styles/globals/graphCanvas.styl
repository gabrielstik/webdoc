.graphCanvas
    position absolute
    top 0
    left 0
    width 100%
    height 100%

.dev
    width 100vw
    height 100vh
    background $black
    // overflow hidden

.graph__text
    position absolute
    top 40%
    left 30px
    color white
    font-size 100px
    font-family 'Averta'
    font-weight bold

.graph__image
    object-fit cover
    width 100%
    height 100%

    user-select none

.graph__discover
    width 80%
    height auto
    position absolute
    left 50%
    top 50%
    mix-blend-mode exclusion
    
    
    user-select none
    transform translateX(-50%) translateY(-50%)

.graph__discoverBlend
    width 80%
    height auto
    position absolute
    left 50%
    top 50%

    transform translateX(-50%) translateY(-50%)